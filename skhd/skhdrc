# changing window focus - colemak
cmd - n : yabai -m window --focus south
cmd - e : yabai -m window --focus north
cmd - m : yabai -m window --focus west
cmd - i : yabai -m window --focus east

# changing window focus - qwerty
cmd - down : yabai -m window --focus south
cmd - up : yabai -m window --focus north
cmd - left : yabai -m window --focus west
cmd - right : yabai -m window --focus east

# toggle window float
shift + cmd - space : yabai -m window --toggle float --grid 4:4:1:1:2:2

# maximize a window
cmd - f : yabai -m window --toggle zoom-fullscreen

# swap windows - colemak
shift + cmd - n : yabai -m window --swap south
shift + cmd - e : yabai -m window --swap north
shift + cmd - m : yabai -m window --swap west
shift + cmd - i : yabai -m window --swap east

# swap windows - qwerty
shift + cmd - down : yabai -m window --swap south
shift + cmd - up : yabai -m window --swap north
shift + cmd - left : yabai -m window --swap west
shift + cmd - right : yabai -m window --swap east

# move window and split - colemak
ctrl + cmd - n : yabai -m window --warp south
ctrl + cmd - e : yabai -m window --warp north
ctrl + cmd - m : yabai -m window --warp west
ctrl + cmd - i : yabai -m window --warp east

# move window and split - qwerty
ctrl + cmd - down : yabai -m window --warp south
ctrl + cmd - up : yabai -m window --warp north
ctrl + cmd - left : yabai -m window --warp west
ctrl + cmd - right : yabai -m window --warp east

# move window to space #
shift + cmd - 1 : yabai -m window --space 1
shift + cmd - 2 : yabai -m window --space 2
shift + cmd - 3 : yabai -m window --space 3
shift + cmd - 4 : yabai -m window --space 4
shift + cmd - 5 : yabai -m window --space 5
shift + cmd - 6 : yabai -m window --space 6
shift + cmd - 7 : yabai -m window --space 7
shift + cmd - 8 : yabai -m window --space 8
shift + cmd - 9 : yabai -m window --space 9
shift + cmd - 0 : yabai -m window --space 10

# move to space - needs  #
cmd - 1 : yabai -m space --focus 1
cmd - 2 : yabai -m space --focus 2
cmd - 3 : yabai -m space --focus 3
cmd - 4 : yabai -m space --focus 4
cmd - 5 : yabai -m space --focus 5
cmd - 6 : yabai -m space --focus 6
cmd - 7 : yabai -m space --focus 7
cmd - 8 : yabai -m space --focus 8
cmd - 9 : yabai -m space --focus 9
cmd - 0 : yabai -m space --focus 10
